{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/Navigator/layers/store/navigator.ts", "src/components/champ/layers/services/servicesHelpers.ts", "src/components/champ/components/CardUser.tsx", "src/components/champ/components/steps/avatarStep.js", "src/components/common/animations/lottie/getLottie.js", "src/components/manager/groupsNreports/store/stat.ts"],
  "exclude": ["node_modules"]
}
